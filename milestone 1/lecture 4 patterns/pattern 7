import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
         
		int n;
		Scanner s=new Scanner(System.in);
		n=s.nextInt();

		for(int i=1;i<=n;i++){
			for(int j=1;j<=n;j++){
				System.out.print((char)(64+i));
			}
			System.out.println();
		}

	
    }
}
